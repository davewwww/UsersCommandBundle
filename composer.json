  "keywords": ["user", "create", "command", "console"],
  "homepage": "https://github.com/davewwww",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Dave Www",
      "email": "davewwwo@gmail.com",
      "homepage": "https://github.com/davewwww"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "symfony/console": "^4.4||^5.0",
    "symfony/security": "^4.4||^5.0",
    "symfony/validator": "^4.4||^5.0",
    "symfony/property-access": "^4.4||^5.0",
    "doctrine/orm": "*"
  },
  "autoload": {
    "psr-4": { "Dwo\\UserCommandsBundle\\": "src/" }
